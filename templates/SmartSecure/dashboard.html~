<script type="text/javascript">

var chart;
$(document).ready(function() {
  $('#id_date').datepicker({ maxDate: "+0D" });
  $('.disable_click').click(function(e) {
    e.preventDefault();
  });

  var ss = "{{apps}}";

  // define the options
  var options = {

    chart: {
      renderTo: 'graph-container'
    },

    title: {
      text: 'App Usage'
    },

    credits: {
      enabled: false
    },
    <!-- subtitle: { -->
    <!--   text: '{{testcase}}' -->
    <!-- }, -->
    xAxis: {
      categories: ss.split(","),
      crosshair: true,
         labels: {
                rotation: -45,
                style: {
                    fontSize: "12px",
                    <!-- fontFamily: 'Verdana, Helvetica, Arial, sans-serif' -->
                }
            }
    },

    yAxis: [{ // Primary yAxis
      labels: {
        formatter: function() {
          return this.value;  
        }
      },
      title: {
        text: 'Time (Min)'
      },
      opposite: true
    }, { // Secondary yAxis
      gridLineWidth: 0,
      title: {
        text: 'Network Used (MBs)'
      },
      labels: {
        formatter: function() {
          return this.value;
        }
      }

    }],

    legend: {
      layout: 'vertical',
      align: 'left',
      x: 80,
      verticalAlign: 'top',   
      y: 120,
      floating: true,
      backgroundColor: '#FFFFFF'
    },

    tooltip: {
      backgroundColor: '#FCFFC5',
      borderColor: 'black',   
      shared: true,
      crosshairs: true
    },


    plotOptions: {
      series: {
        cursor: 'pointer',
        point: {
          events: {
            click: function() {
              hs.htmlExpand(null, {
                pageOrigin: { 
                  x: this.pageX,
                  y: this.pageY
                },
                headingText: this.series.name,
                maincontentText: Highcharts.dateFormat('%A, %b %e, %Y', this.x) +':<br/> '+
                  this.y +' ',
                width: 300
              });
            }
          }
        },
        marker: {
          lineWidth: 1
        }
      }
    },
    series: [
      {
        name: 'duration',
        lineWidth: 2,
        dataLabels: {
          enabled: true,
          rotation: -90,
          color: '#FFFFFF',
          align: 'right',
          format: '{point.y:.1f}', // one decimal
          y: 10, // 10 pixels down from the top
            style: {
              fontSize: '13px',
              fontFamily: 'Verdana, sans-serif'
              }
         },
         type: 'column',
         tooltip: {
                valueSuffix: ' Minute'
            }

         
      },
      {
        name: 'Total',
        lineWidth: 2,
        type: 'spline',
        yAxis: 1,
        tooltip: {
                valueSuffix: ' MB'
            }
      },
      {
        name: 'Received',
        lineWidth: 2,
        type: 'spline',
        yAxis: 1,
        tooltip: {
                valueSuffix: ' MB'
            }
      },

      {
        name: 'Transmitted',
        lineWidth: 2,
        type: 'spline',
        yAxis: 1,
        tooltip: {
                valueSuffix: ' MB'
            }
      }
    ]

  };

{% if access_chart %}
  options.series[0].data = {{access_chart}};
  options.series[1].data = {{total}};
  options.series[2].data = {{received}};
  options.series[3].data = {{transmitted}};

  chart = new Highcharts.Chart(options);

{% endif %}

});

</script>



{% endblock %}

